{
	"name": "lorem-whatever",
	"displayName": "Lorem Whatever",
	"description": "A Lorem generator, doing William Gibson or Goethe or Buzzwords style none-sense filler text.",
	"version": "0.0.1",
	"publisher": "Daniel Keller",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:loremWhatever.loremGibson",
		"onCommand:loremWhatever.loremBuzzword",
		"onCommand:loremWhatever.loremConstitution",
		"onCommand:loremWhatever.loremBasel3",
		"onCommand:loremWhatever.loremFaust",
		"onCommand:loremWhatever.loremGenesis"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "loremWhatever.loremGibson",
				"title": "Lorem Gibson"
			},
			{
				"command": "loremWhatever.loremBuzzword",
				"title": "Lorem Buzzword"
			},
			{
				"command": "loremWhatever.loremConstitution",
				"title": "Lorem Constitution"
			},
			{
				"command": "loremWhatever.loremBasel3",
				"title": "Lorem Basel3"
			},
			{
				"command": "loremWhatever.loremFaust",
				"title": "Lorem Faust"
			},
			{
				"command": "loremWhatever.loremGenesis",
				"title": "Lorem Genesis"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"tslint": "^5.8.0",
		"typescript": "^3.1.4",
		"vscode": "^1.1.26"
	},
	"dependencies": {
		"markov-strings": "^2.0.1"
	}
}
